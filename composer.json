{
	"name": "redaxmedia/redaxscript-website-sync",
	"description": "Yet another cronjob for Redaxscript's website",
	"version": "3.1.0",
	"homepage": "https://redaxscript.com",
	"license": "GPL-3.0",
	"repositories":
	[
		{
			"type": "git",
			"url": "https://github.com/redaxmedia/redaxscript-template.git"
		}
	],
	"require":
	{
		"php": ">5.4"
	},
	"require-dev":
	{
		"redaxmedia/redaxscript": "dev-master",
		"redaxmedia/redaxscript-template": "dev-master"
	},
	"scripts":
	{
		"post-install-cmd":
		[
			"cd vendor/redaxmedia/redaxscript && composer install"
		],
		"post-update-cmd":
		[
			"@post-install-cmd"
		]
	}
}
